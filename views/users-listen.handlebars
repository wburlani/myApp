<h1>Users</h1>
{{#each users}}
<div class="card" col-md-12">
	<div class="row">
<div class="col-md-4">
  <img src="{{id}}.png" class="card-img-top" alt="...">
</div>
  <div class="card-body col-md-6">
  	<h4>Photograph</h4>
  	<h6>
    Id: {{id}}<br>
    Name: {{name}}<br>
    Email: {{email}}<br>
    Created: {{#formatDate createdAt}}{{/formatDate}}<br>
    Updated: {{#formatDate updatedAt}}{{/formatDate}}
    </h6>
  </div>
</div>
</div>
{{/each}}
